/*日历  */

.date-show {
  position: relative;
  width: 250rpx;
  font-family: PingFang-SC-Regular;
  font-size: 30rpx;
  color: #282828;
  text-align: center;
  margin: 30rpx auto 10rpx;
}

 .lt-arrow{
  position: absolute;
  top: -2px;
  width: 20px;
  height: 20px;
} 
.rt-arrow {
  position: absolute;
  top: -2px;
  width: 20px;
  height: 20px;
  right:-110rpx;
  transform:rotate(270deg);
}
.lt-arrow image, .rt-arrow image {
  width: 20px;
  height: 20px;
}

.lt-arrow {
  left: -110rpx;
 transform:rotate(90deg);
}

.header {
  font-size: 0;
   padding: 0 24rpx; 
}

.header>view {
  display: inline-block;
  width: 14.285%;
  color: #333;
  font-size: 30rpx;
  text-align: center;
  /* border-bottom: 1px solid #d0d0d0; */
