.menu-list {
  /* position: absolute; */
  left: 0;
  top: 0;
  width: inherit;
  height: inherit;
}

.menu-item {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  font-weight: 500;
}

.menu-circle-item {
  -webkit-transform-origin: 50% 48vw;
  transform-origin: 50% 48vw;
  margin: 0 auto;
  margin-top: 2vw;
  position: relative;
  height: 16vw;
  width: 16vw;
  background-color: #ec881d;
  text-align: center;
  border-radius: 50%;
  z-index: 999;
  border: 5px solid #f3f2f8;
  line-height: 16vw;
  box-shadow: 0 0 3vw 0 #999;
}

.image-style {
  height: 16vw;
  width: 16vw;
  color: #f00;
  z-index: 999;
}

.rotate1 {
  -webkit-transform: rotate(-75deg);
