page {
  background-color: #ececec;
  width: 100%;
  height: 100%;
  padding: 0;
  margin: 0;
}

.container {
  height: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
  -webkit-text-size-adjust: none;
  -webkit-user-select: none;
  font-size: 35rpx;
  color: #333;
  font-family: Helvetica, Arial, "Helvetica Neue", "Droid Sans", "Microsoft YaHei", sans-serif;
}

.canvas {
  width: 100%;
  height: 200px;
  display: flex;
  justify-content: center;
  align-items: center;
}

.weui-media-box-height {
  padding-top: 3px;
  position: relative;
}

.weui-media-box-up {
  padding-top: 13px;
  padding-bottom: 20px;
}

.mbTxing {
  width: 100%;
  background: #004299;
  padding-bottom: 10rpx;
  color: #ff8e28;
}

.picker-s {
  overflow: hidden;
  width: 92%;
  border: 2rpx #f1f1f1 solid;
  background: #fff;
  margin: 0 auto;
  border-radius: 5px;
  font-size: 36rpx;
  text-align: center;
}

.menu-item {
  width: 100%;
}

.kefu {
  margin: auto;
  color: #fff;
  width: 50px;
  height: 50px;
  background: #de8a12;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.weui-cell_primary-color-font {
  color: #999;
  font-size: 12px;
}

/* //颜色 */

.background-white {
  background-color: #fff;
}
